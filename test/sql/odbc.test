# name: test/sql/odbc.test
# description: test odbc extension
# group: [odbc]

# Require statement will ensure this test is run with this extension loaded
require odbc

# Confirm the extension works with connection string
query I
FROM odbc_query('Driver=DuckDB Driver;Database=:memory:','SELECT 42');
----
42

# Confirm the extension works with DSN
query I
FROM odbc_query('DuckDB','SELECT 42');
----
42